{
  "path": "/directory-move",
  "executor": "bash",
  "method": "post",
  "args": [
    {
      "name": "sourcePath",
      "type": "json",
      "source": "body",
      "description": "The source path of the directory to be moved."
    },
    {
      "name": "destinationPath",
      "type": "json",
      "source": "body",
      "description": "The destination path where the directory should be moved to."
    }
  ],
  "responses": [
    {
      "statusCode": 200,
      "description": "Directory moved successfully",
      "type": "string"
    },
    {
      "statusCode": 400,
      "description": "Bad request",
      "type": "string"
    }
  ]
}