{
  "path": "/directory-create",
  "executor": "bash",
  "method": "post",
  "args": [
    {
      "name": "workingDirectory",
      "type": "json",
      "source": "body",
      "description": "The base path where the new directory should be created."
    },
    {
      "name": "directoryName",
      "type": "json",
      "source": "body",
      "description": "The name of the new directory."
    }
  ],
  "responses": [
    {
      "statusCode": 200,
      "description": "Directory created successfully",
      "type": "string"
    },
    {
      "statusCode": 400,
      "description": "Bad request",
      "type": "string"
    }
  ]
}